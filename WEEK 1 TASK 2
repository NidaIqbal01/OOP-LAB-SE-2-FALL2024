#include<iostream>
using namespace std;
class car {
public:
 string name;
 int speed;
 string model;
 void displayInfo() {
 cout << "Name: " <<name<<"\n"<<"Model Name: "<<model<<"\n"<< "Speed: " << speed << " km/h" << endl;
 }
};
int main()
{
car c1;
c1.name="BMW";
c1.speed=100;
c1.model="TE300";
c1.displayInfo();
return 0;
}
